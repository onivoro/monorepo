{
  "name": "lib-isomorphic-fhir",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/isomorphic/fhir/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/isomorphic/fhir",
        "main": "libs/isomorphic/fhir/src/index.ts",
        "tsConfig": "libs/isomorphic/fhir/tsconfig.lib.json",
        "assets": [
          "libs/isomorphic/fhir/README.md",
          "libs/isomorphic/fhir/LICENSE",
          "libs/isomorphic/fhir/package.json"
        ],
        "updateBuildableProjectPackageJsonDependencies": true
      }
    },
    "nx-release-publish": {
      "executor": "@nx/js:release-publish"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/isomorphic/fhir/jest.config.ts"
      }
    }
  },
  "tags": []
}
