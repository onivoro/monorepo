{
  "name": "app-server-datavore",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/server/datavore/src",
  "projectType": "application",
  "targets": {
    "build-client": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{projectRoot}/src/assets/scripts"],
      "options": {
        "target": "web",
        "compiler": "tsc",
        "outputPath": "apps/server/datavore/src/assets/scripts",
        "webpackConfig": "apps/server/datavore/webpack.client.config.js"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["build-client"],
      "options": {
        "outputPath": "dist/apps/server/datavore",
        "webpackConfig": "apps/server/datavore/webpack.config.js"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": [
        "build"
      ],
      "options": {
        "buildTarget": "app-server-datavore:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "app-server-datavore:build:development"
        },
        "production": {
          "buildTarget": "app-server-datavore:build:production"
        }
      }
    },
    "nx-release-publish": {
      "executor": "@nx/js:release-publish",
      "options": {
        "packageRoot": "dist/apps/server/datavore"
      }
    },
    "local": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./tools/scripts/serve-datavore.sh"
      }
    }
  },
  "tags": []
}
