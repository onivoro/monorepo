{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "module": "NodeNext",
    "types": ["node"],
    "lib": [
      "es2023"
    ],
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "NodeNext",
    "skipLibCheck": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "target": "es2023"
  },
  "references": []
}
