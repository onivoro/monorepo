  "scripts": {
    "nx": "nx",
    "storybook": "npx nx storybook lib-browser-react-components",
    "random": "echo 'pass a numeric arg to change the default length of 16' & bun tools/scripts/rando.ts",
    "db:up:local": "cd docker/dev && docker compose up -d --remove-orphans",
    "db:down:local": "cd docker/dev && docker compose down",
    "fmt": "nx format --fix",
    "setup:mcp": "node scripts/setup-mcp-config.js",
    "setup:mcp:dry-run": "node scripts/setup-mcp-config.js --dry-run",
    "lib-axios-b2b:deploy": "nx build lib-axios-b2b && cp .npmrc dist/libs/axios/b2b && cd dist/libs/axios/b2b && npm publish --access public",
    "lib-axios-rx:deploy": "nx build lib-axios-rx && cp .npmrc dist/libs/axios/rx && cd dist/libs/axios/rx && npm publish --access public",
    "test": "npm run test:unit",
    "test:unit": "nx run-many -t test --passWithNoTests",
    "typeorm": "ts-node --project=tsconfig.server.json -r dotenv-flow/config -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test:ui-botvim": "node ui_monitoring/botvim/agents.spec.js",
    "watch:ui-botvim": "nodemon --ext js,ts,tsx --watch apps/browser/botvim/src --exec 'node ui_monitoring/botvim/agents.spec.js'"
  },
