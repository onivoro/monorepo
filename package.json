  "scripts": {
    "nx": "nx",
    "datavore:build": "npx nx run app-server-datavore:build:production",
    "datavore:containerize:amd": "docker build --build-arg=\"APP_DIST=dist/apps/server/datavore\" -f docker/prod/server/Dockerfile --platform linux/amd64 -t icedlee337/datavore:amd64 . && echo $DOCKER_PASSWORD | docker login --username $DOCKER_USERNAME --password-stdin && docker push icedlee337/datavore:amd64",
    "datavore:containerize:arm": "docker build --build-arg=\"APP_DIST=dist/apps/server/datavore\" -f docker/prod/server/Dockerfile --platform linux/arm64/v8 -t icedlee337/datavore:arm64v8 . && echo $DOCKER_PASSWORD | docker login --username $DOCKER_USERNAME --password-stdin && docker push icedlee337/datavore:arm64v8",
    "datavore:deploy": "npm run datavore:build && npm run datavore:containerize:arm && npm run datavore:containerize:amd",
    "random": "echo 'pass a numeric arg to change the default length of 16' & bun tools/scripts/rando.ts",
    "db:up:local": "cd docker/dev && docker compose up -d --remove-orphans",
    "db:down:local": "cd docker/dev && docker compose down",
    "fmt": "nx format --fix",
    "setup:mcp": "node scripts/setup-mcp-config.js",
    "setup:mcp:dry-run": "node scripts/setup-mcp-config.js --dry-run",
    "test": "npm run test:unit",
    "test:unit": "nx run-many -t test --passWithNoTests",
    "release:patch": "nx release version patch && npm run release:complete",
    "release:minor": "nx release version minor && npm run release:complete",
    "release:major": "nx release version major && npm run release:complete",
    "release:version": "nx release version",
    "release:build": "nx run-many -t build --projects='lib-server-*,lib-isomorphic-*,app-server-datavore'",
    "release:commit": "git add . && git commit -m \"chore(release): v$(node -p \"require('./libs/server/common/package.json').version\")\"",
    "release:tag": "git tag \"v$(node -p \"require('./libs/server/common/package.json').version\")\"",
    "release:publish": "nx release publish",
    "release:complete": "npm run release:build && npm run release:commit && npm run release:tag && npm run release:publish",
    "release:dry-run": "nx release version patch --dry-run"
  },
